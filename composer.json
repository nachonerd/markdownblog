{
    "name": "nachonerd/markdownblog",
    "description": "Minimalist Markdown Blog",
    "type": "project",
    "keywords": ["PHP", "Markdown", "Blog", "Framework"],
    "license": "GPL-3.0",
    "autoload": {
        "psr-4": {"NachoNerd\\MarkdownBlog\\": "src/"}
    },
    "authors": [
        {
            "name": "Ignacio Rodrigo Galieri",
            "email": "irgalieri@gmail.com",
            "homepage": "http://elnachonerd.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "irgalieri@gmail.com",
        "issues": "https://github.com/nachonerd/markdownblog/issues"
    },
    "require": {
        "php": ">=5.4.0",
        "silex/silex": "1.3.*",
        "cebe/markdown": "1.1.*",
        "twig/twig": "1.21.*",
        "deralex/yaml-config-service-provider": "1.0.x",
        "nachonerd/silex-finder-provider": "^1.1",
        "nachonerd/silex-markdown-provider": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.7.*",
        "squizlabs/php_codesniffer": "2.*",
        "symfony/browser-kit": "^2.7",
        "symfony/css-selector": "^2.7"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.x-dev-stable",
            "dev-development": "0.0.x-dev-unstable"
        }
    }
}
